public class Fibonacci 
{
	public void Calculo()
	{
		int a, b, c;
    		a = 0;
    		b = 1;

		print(a);
    		print(b);

    		while(b < 100) {
        		     c = a + b;
        		     if(c < 100) {
	                   print(c);
        		}
        		    a = b;
        		    b = c;
    		}
	}

	void main() 
	{
		Calculo();    	 	
	}
}
